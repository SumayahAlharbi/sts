{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "^4.6",
        "ckeditor/ckeditor": "^4.13",
        "consoletvs/charts": "6.5.1",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "glorand/laravel-model-settings": "^3.5",
        "itsgoingd/clockwork": "^4.0",
        "jeremykenedy/laravel-logger": "^3.0",
        "jimmyjs/laravel-report-generator": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^3.4",
        "laravel/telescope": "~1.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7",
        "laravolt/avatar": "^2.1",
        "league/csv": "^9.1",
        "league/oauth2-client": "^2.4",
        "microsoft/microsoft-graph": "^1.11",
        "orangehill/iseed": "^2.6",
        "pragmarx/health": "^0.10.2",
        "pragmarx/version": "^0.2.8",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.2",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-backup": "^5.11",
        "spatie/laravel-permission": "^2.23",
        "springscs/cas": "^1.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^6.1",
        "vlucas/phpdotenv": "^2.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "deployer/deployer": "^6.7",
        "deployer/recipes": "^6.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.7",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
